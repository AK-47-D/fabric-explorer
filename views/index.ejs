    <script href="source/inspinia/js/plugins/iCheck/icheck.min.js"></script>

    <!-- Peity -->
    <script href="source/inspinia/js/demo/peity-demo.js"></script>
    
    
    
    <script src="source/grap/vis.js"></script>
    
    
<script type="text/javascript">
  // create an array with nodes
  var nodes = new vis.DataSet([
    {id: 1, label: 'org 1',color: '#7BE141'},
    {id: 2, label: 'org 2',color: '#7BE141'}
  ]);

  // create an array with edges
  var edges = new vis.DataSet([
    {from: 1, to: 2}
  ]);

  // create a network
  var container = document.getElementById('main');
  var data = {
    nodes: nodes,
    edges: edges
  };
  var options = {};
  var network = new vis.Network(container, data, options);
</script>
    
    
<script type="text/javascript">
  // create an array with nodes
  var nodes1 = new vis.DataSet([
    {id: 1, label: 'peer 1',color: '#7BE141'},
    {id: 2, label: 'peer 2',color: '#7BE141'},
    {id: 3, label: 'peer 3',color: '#7BE141'}

  ]);

  // create an array with edges
  var edges1 = new vis.DataSet([
    {from: 1, to: 3},
    {from: 1, to: 2},
    {from: 2, to: 3}
  ]);

  // create a network
  var container1 = document.getElementById('main1');
  var data1 = {
    nodes: nodes1,
    edges: edges1
  };
  var options1 = {};
  var network1 = new vis.Network(container1, data1, options1);
</script>    
    

